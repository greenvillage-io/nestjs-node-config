{
    "name": "nestjs-node-config",
    "version": "0.0.1",
    "description": "The nestjs node-config package",
    "author": "Alex Hermann <alex.hermann@pop-code.com>",
    "repository": "https://github.com/Pop-Code/nestjs-node-config",
    "main": "build/index.js",
    "scripts": {
        "build": "rm -Rf build && tsc -p tsconfig.build.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:e2e:debug": "node --inspect-brk  node_modules/.bin/jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "config": "3.1.0"
    },
    "peerDependencies": {
        "@nestjs/common": "^6.1.1"
    },
    "devDependencies": {
        "@nestjs/common": "6.2.4",
        "@nestjs/testing": "6.2.4",
        "@types/jest": "24.0.13",
        "codecov": "3.5.0",
        "jest": "24.8.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.2",
        "ts-jest": "24.0.2",
        "ts-node": "8.1.0",
        "tsconfig-paths": "3.8.0",
        "typedoc": "0.14.2",
        "typescript": "3.4.5"
    }
}
